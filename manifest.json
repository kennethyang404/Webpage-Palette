{
  "name": "Webpage Palette",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Extract a palette from your webpage!",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [ "activeTab", 
                   "clipboardWrite" ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
